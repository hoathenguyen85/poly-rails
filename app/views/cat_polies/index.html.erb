<p id="notice"><%= notice %></p>

<h1>Listing Cat Polies</h1>

<table>
  <thead>
    <tr>
      <th>Dog poly</th>
      <th>Fish poly</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cat_polies.each do |cat_poly| %>
      <tr>
        <td><%= cat_poly.dog_poly %></td>
        <td><%= cat_poly.fish_poly %></td>
        <td><%= link_to 'Show', cat_poly %></td>
        <td><%= link_to 'Edit', edit_cat_poly_path(cat_poly) %></td>
        <td><%= link_to 'Destroy', cat_poly, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cat poly', new_cat_poly_path %>
